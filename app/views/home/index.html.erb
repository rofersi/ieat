<h1>Home#index</h1>
<p>Hello <%= @email  %> </p>


<% if user_signed_in? %>
  
  <% if current_user.restaurant? %>
    <%= render 'restaurant' %>
  <% elsif current_user.customer? %>
    <%= render 'customer' %>
  <% elsif current_user admin? %>
    <%= render 'admin' %>       
  <% end %>
  
  
<% end %>


